                                Style="{ThemeResource BodyTextBlockStyle}"
                                Text="{TemplateBinding Label}"
                                TextAlignment="Left"
                                TextTrimming="Clip"
                                TextWrapping="NoWrap"
                                Visibility="Collapsed" />
                            <contract6Present:TextBlock
